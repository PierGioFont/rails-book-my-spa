<h1>Spas#index</h1>

<div class="container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">
        <% @spas.each do |spa|  %>
            <div class="col-sm-4 spa-card">
              <%= link_to spa_path(spa) do %>
                <h2><%= spa.name %></h2>
              <% end %>
              <%#= render 'shared/stars', rating: spa.average_rating%>
              <p><%= spa.address %></p>
              <p><%= spa.description %></p>
            </div>
        <% end %>
    </div>
  </div>
</div>
