<%= simple_form_for [@spa, @booking] do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :massage_id, collection: @spa.massages, selected: @massage %>
    <%= f.input :date %>
    <%= f.input :time_in %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
